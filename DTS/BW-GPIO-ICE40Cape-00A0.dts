/dts-v1/;
/plugin/;

/ {
	compatible = "ti,beaglebone", "ti,beaglebone-black";

	/* Identification */
	part-number = "BW-GPIO-ICE40Cape";
	version = "00A0";

	fragment@0 {
		target = <&ocp>;
		__overlay__ {
			gpio_ice40: gpio-controller@1000000 {
                                compatible = "beagle,ice40-gpio";
				reg = <0x1000000>;

				#gpio-cells = <2>;
				gpio-controller;

				nr-gpios = <64>;
			};
		};
	};
};
